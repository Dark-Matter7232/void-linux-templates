do_install() {                              
        vman usr/share/man/man1/na.1.gz
        vman usr/share/man/man1/nse.1.gz
        vman usr/man/share/man/man1/nstk.1.gz
        vdoc usr/share/doc/ns2/*
        vbin bin/calcdest
	vbin bin/dec-tr-stat
	vbin bin/epa-tr-stat
        vbin bin/nlanr-tr-stat
        vbin bin/ns
        vbin bin/nse
        vbin bin/nstk
        vbin bin/setdest
        vbin bin/ucb-tr-stat
}
